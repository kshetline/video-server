{
  "name": "video-ui",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Kerry Shetline <kerry@shetline.com>",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start-https": "ng serve --proxy-config proxy-https.conf.json --ssl --ssl-key ./cert/localhost.key --ssl-cert ./cert/localhost.crt",
    "start-win-https": "ng serve --proxy-config proxy-https.conf.json --ssl --ssl-key .\\cert\\localhost.key --ssl-cert .\\cert\\localhost.crt",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint '**/*.ts' '**/*.js'",
    "deploy": "npm run build && cd server && npm run build && cd .. && rimraf --glob \"../docker-nodejs/nodejs/public/*\" && copyfiles -u 2 dist/video-ui/**/* ../docker-nodejs/nodejs/public/ && copyfiles -u 2 server/dist/app* ../docker-nodejs/nodejs/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@fortawesome/angular-fontawesome": "^0.12.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@tubular/math": "^3.3.1",
    "@tubular/util": "^4.15.1",
    "dashjs": "^4.7.2",
    "jimp": "^0.22.10",
    "primeicons": "^6.0.1",
    "primeng": "~15.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular/cli": "~15.2.10",
    "@angular/compiler-cli": "^15.2.0",
    "@types/copyfiles": "^2.4.2",
    "@types/follow-redirects": "^1.14.2",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.51.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^4.9.4"
  }
}
