{
  "name": "video-ui",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Kerry Shetline <kerry@shetline.com>",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start-https": "ng serve --proxy-config proxy-https.conf.json --ssl --ssl-key ./cert/localhost.key --ssl-cert ./cert/localhost.crt",
    "start-win-https": "ng serve --proxy-config proxy-https.conf.json --ssl --ssl-key .\\cert\\localhost.key --ssl-cert .\\cert\\localhost.crt",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint --ignore-pattern '.angular/' --ignore-pattern '**/tsc-out/' --ignore-pattern '**/dist/'",
    "deploy": "npm run build && cd server && npm run build && cd dist && npm init -y && npm i sqlite3 && cd .. && cd .. && rimraf --glob \"../docker-nodejs/nodejs/public/*\" && copyfiles -u 2 dist/video-ui/**/* ../docker-nodejs/nodejs/public/ && copyfiles -u 2 server/dist/**/* ../docker-nodejs/nodejs/",
    "deploy-mac": "npm run build && cd server && npm run build && cd dist && npm init -y && npm i sqlite3 && cd .. && cd .. && npm run deploy-move-mac",
    "deploy-move-mac": "rimraf \"/Users/kshetline/Sites/shetline.org/nodejs/public\" && mkdir -p ~kshetline/Sites/shetline.org/nodejs/public && cp -R dist/video-ui/ ~kshetline/Sites/shetline.org/nodejs/public/ && cp -R server/dist/ ~kshetline/Sites/shetline.org/nodejs/",
    "postinstall": "patch-package"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.10",
    "@angular/common": "^19.2.10",
    "@angular/compiler": "^19.2.10",
    "@angular/core": "^19.2.10",
    "@angular/forms": "^19.2.10",
    "@angular/platform-browser": "^19.2.10",
    "@angular/platform-browser-dynamic": "^19.2.10",
    "@angular/router": "^19.2.10",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@primeng/themes": "^19.1.2",
    "@tubular/math": "^3.4.0",
    "@tubular/util": "^4.19.0",
    "canvas": "^2.11.2",
    "dashjs": "^5.0.3",
    "jimp": "^0.22.10",
    "primeicons": "^7.0.0",
    "primeng": "^19.1.2",
    "resemblejs": "^5.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.11",
    "@angular/cli": "^19.2.11",
    "@angular/compiler-cli": "^19.2.10",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@types/copyfiles": "^2.4.2",
    "@types/follow-redirects": "^1.14.2",
    "@types/jasmine": "~5.1.0",
    "@types/resemblejs": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.29.0",
    "eslint-plugin-chai-friendly": "^1.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "patch-package": "^8.0.0",
    "rimraf": "^5.0.5",
    "typescript": "~5.8.3"
  },
  "overrides": {
    "vite": "6.0.11"
  }
}
